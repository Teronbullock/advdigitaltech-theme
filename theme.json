{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultPalette": false,
      "palette": [
        {
          "name": "TH-Gray",
          "slug": "th-gray",
          "color": "#f5f5f7"
        },
        {
          "name": "TH-Blue",
          "slug": "th-blue",
          "color": "#125FB0"
        },
        {
          "name": "TH-Red",
          "slug": "th-red",
          "color": "#F63636"
        },
        {
          "name": "TH-White",
          "slug": "th-white",
          "color": "#FBFBFB"
        },
        {
          "name": "TH-Black",
          "slug": "th-black",
          "color": "#000000"
        }
      ]
    },
    "layout": {
      "contentSize": "1024px",
      "wideSize": "1400px"
    },
    "spacing": {
      "units": ["%", "px", "em", "rem", "vh", "vw"],
      "margin": true,
      "padding": true
    },
    "typography": {
      "defaultFontSizes": false,
      "fluid": {
        "minViewportWidth": "275px",
        "maxViewportWidth": "1200px"
      },
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "fontFamily": "Montserrat sans-serif",
          "name": "Montserrat",
          "slug": "montserrat",
          "fontFace": [
            {
              "fontFamily": "Montserrat",
              "fontStyle": "normal",
              "fontWeight": "400",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/montserrat/montserrat-v26-latin-regular.woff2"
              ]
            },
            {
              "fontFamily": "Montserrat",
              "fontStyle": "italic",
              "fontWeight": "400",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/montserrat/montserrat-v26-latin-italic.woff2"
              ]
            },
            {
              "fontFamily": "Montserrat",
              "fontStyle": "normal",
              "fontWeight": "700",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/montserrat/montserrat-v26-latin-700.woff2"
              ]
            },
            {
              "fontFamily": "Montserrat",
              "fontStyle": "italic",
              "fontWeight": "700",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/montserrat/montserrat-v26-latin-700italic.woff2"
              ]
            },
            {
              "fontFamily": "Montserrat",
              "fontStyle": "normal",
              "fontWeight": "900",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/montserrat/montserrat-v26-latin-900.woff2"
              ]
            },
            {
              "fontFamily": "Montserrat",
              "fontStyle": "italic",
              "fontWeight": "900",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/montserrat/montserrat-v26-latin-900italic.woff2"
              ]
            }
          ]
        },
        {
          "fontFamily": "Open Sans sans-serif",
          "name": "Open Sans",
          "slug": "open-sans",
          "fontFace": [
            {
              "fontFamily": "Open Sans",
              "fontStyle": "normal",
              "fontWeight": "300",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/open-sans/open-sans-v40-latin-300.woff2"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontStyle": "italic",
              "fontWeight": "300",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/open-sans/open-sans-v40-latin-300italic.woff2"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontStyle": "normal",
              "fontWeight": "400",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/open-sans/open-sans-v40-latin-regular.woff2"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontStyle": "italic",
              "fontWeight": "400",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/open-sans/open-sans-v40-latin-italic.woff2"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontStyle": "normal",
              "fontWeight": "500",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/open-sans/open-sans-v40-latin-500.woff2"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontStyle": "italic",
              "fontWeight": "500",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/open-sans/open-sans-v40-latin-500italic.woff2"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontStyle": "normal",
              "fontWeight": "700",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/open-sans/open-sans-v40-latin-700.woff2"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontStyle": "italic",
              "fontWeight": "700",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/open-sans/open-sans-v40-latin-700italic.woff2"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontStyle": "normal",
              "fontWeight": "800",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/open-sans/open-sans-v40-latin-800.woff2"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontStyle": "italic",
              "fontWeight": "800",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/open-sans/open-sans-v40-latin-800italic.woff2"
              ]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "fluid": {
            "min": "0.875rem",
            "max": "2rem"
          },
          "name": "Small",
          "size": "1rem",
          "slug": "small"
        },
        {
          "fluid": {
            "min": "1rem",
            "max": "1.25rem"
          },
          "name": "Medium",
          "size": "1.25rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "min": "1.25rem",
            "max": "1.5rem"
          },
          "name": "Large",
          "size": "1.5rem",
          "slug": "large"
        },
        {
          "fluid": {
            "min": "2rem",
            "max": "3.75rem"
          },
          "name": "Extra Large",
          "size": "3rem",
          "slug": "x-large"
        }
      ]
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--th-gray)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--open-sans)"
    },
    "blocks": {
      "core/heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)"
        }
      }
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)"
        }
      }
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header"
    },
    {
      "area": "footer",
      "name": "footer"
    }
  ]
}
